<Window x:Class="Screens.AddWarehouse"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:Screens"
        mc:Ignorable="d"
        Title="AddWarehouse" Height="300" Width="400" ResizeMode="NoResize">
    <Grid>
        <TextBlock Text="Depo giriniz:" FontWeight="DemiBold" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="50,95,0,0"/>
        <TextBox materialDesign:HintAssist.Hint="Depo adı" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,90,0,0" Width="200" Name="WarehouseName" />
        <Button x:Name="SaveBtn" Content="Kaydet" Width="200" Margin="0,40,0,0" Click="SaveBtn_Click"/>
    </Grid>
</Window>
